<div id="fileupload">
  <form method="POST" enctype="multipart/form-data" id="form1" action="/uploads">
    <div class="row">
      <label class="filetoUpload"> Select Files to Upload </label><br />
      <input type="file" name="files[]" multiple>
    </div>    
    <div id="fileName"></div>
    <div id="fileSize"></div>
    <div id="fileType"></div>
    <div class="row">
      <input type="button" onclick="uploadFile()" value="Upload" />
    </div>

    <div id="progressNumber"></div>
  </form>
</div>

